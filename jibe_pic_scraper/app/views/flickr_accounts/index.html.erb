<p id="notice"><%= notice %></p>

<h1>Listing Flickr Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Access token</th>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flickr_accounts.each do |flickr_account| %>
      <tr>
        <td><%= flickr_account.access_token %></td>
        <td><%= flickr_account.username %></td>
        <td><%= link_to 'Show', flickr_account %></td>
        <td><%= link_to 'Edit', edit_flickr_account_path(flickr_account) %></td>
        <td><%= link_to 'Destroy', flickr_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flickr account', new_flickr_account_path %>

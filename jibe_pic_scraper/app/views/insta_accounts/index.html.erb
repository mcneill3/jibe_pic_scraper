<table>
  <thead>
    <tr>
      <th>Username</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insta_accounts.each do |account| %>
      <tr>
        <td><%= account.username %></td>
        <td><%= link_to 'Show', account %></td>
        <td><%= link_to 'Edit', edit_insta_account_path(account) %></td>
        <td><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
